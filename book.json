{
    "description": "My Pedia",
    "title": "Geekern",
    "author": "andyzyy@gmail.com",
    "plugins": [
        "uml",
        "editlink",
        "atoc",
        "katex",
        "highlight",
        "expandable-chapters",
        "jira",
        "ad",
        "add-js-css",
        "mindmap-7dtime",
        "include-codeblock",
        "ace"
    ],
    "pluginsConfig": {
        "editlink": {
